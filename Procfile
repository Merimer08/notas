web: sh -lc 'set -eux; echo "DB_DATABASE=$DB_DATABASE"; mkdir -p "$(dirname "$DB_DATABASE")"; [ -f "$DB_DATABASE" ] || touch "$DB_DATABASE"; chmod -R 777 /workspace/storage bootstrap/cache; php artisan optimize:clear; php artisan storage:link || true; php artisan migrate --force --seed; php artisan serve --host 0.0.0.0 --port $PORT'  